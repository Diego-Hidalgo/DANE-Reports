<Page x:Class="DANE_Reports.ui.TableScreen"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DANE_Reports.ui"
      mc:Ignorable="d" 
      d:DesignHeight="325" d:DesignWidth="600" ShowsNavigationUI="False"
      Title="TableScreen"
      Loaded="Page_Loaded">

    <Grid Background="White">
        <DataGrid Name = "tableData" Background="White" d:ItemsSource="{d:SampleData ItemCount=5}"/>
    </Grid>
</Page>
